{
  "name": "qr-hunt-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --report-json",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit --coverage",
    "bundle-analyze": "webpack-bundle-analyzer dist/report.json",
    "stylelint": "stylelint **/*.{vue,scss} --fix"
  },
  "dependencies": {
    "core-js": "3.15.2",
    "node-snackbar": "0.1.16",
    "qr-scanner": "1.2.0",
    "register-service-worker": "1.7.2",
    "vue": "2.6.14",
    "vue-router": "3.5.2",
    "vue2-google-maps": "0.10.7",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@types/googlemaps": "3.43.3",
    "@types/jest": "26.0.23",
    "@types/qrcode": "1.4.0",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-pwa": "4.5.13",
    "@vue/cli-plugin-typescript": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.2.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.29.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "5.2.3",
    "file-loader": "6.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "node-sass": "6.0.1",
    "prettier": "1.19.1",
    "sass": "1.35.1",
    "sass-loader": "8.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-recess-order": "2.4.0",
    "stylelint-config-standard": "20.0.0",
    "typescript": "3.9.10",
    "vue-template-compiler": "2.6.14",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "engines": {
    "node": "12.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,vue}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,ts}": "vue-cli-service lint",
    "*.{vue,scss}": "stylelint --fix"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order"
    ]
  }
}
